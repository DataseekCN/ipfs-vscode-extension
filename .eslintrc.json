{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "extends": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "prettier/prettier": "error"
  },
  "ignorePatterns": ["out", "dist", "**/*.d.ts"]
}
